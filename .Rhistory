search_test <- function(inventor, inventor_boolean = "NULL", query = "NULL", type = "NULL", boolean = "NULL"){
andlink <- "+%26%26+"
search <- query # avoiding confusion
#build the inventor query and add the bridge link to text search
query <- lens_inventor(inventor, inventor_boolean)
query <- paste0(query, andlink)
##add the text search to the query
search <- lens_urls(search, type, boolean)
search <- stringr::str_split(search, "=", n = 2) %>% .[[2]] #potential point of weakness?
out <- paste0(query, search)
return(out)
}
